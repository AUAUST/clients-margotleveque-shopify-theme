<main>
  <!-- Header -->
  <header>
    {% include 'menu' %}
    <!-- Project List -->
    <ul class="font-nav"></ul>
    <!-- Contact -->
    <p
      class="contact-links font-nav"
    >
      {% for link in linklists.footer.links -%}
        <a
          href="{{ link.url }}"
          target="_blank"
          rel="noreferrer"
          class="font-nav"
        >
          {{ link.title }} 
        </a>
        
      {%- endfor %}
    </p>
  </header>
  <!-- Projects -->
  <div
    id="projects"
  >
    {{ content_for_index }}
  </div>
</main>
